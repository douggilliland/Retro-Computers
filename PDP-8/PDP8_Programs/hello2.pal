*10
index,	hello-1

*200
	jmp start
/ hello,	.+1; 'Hello, World!'; 13d; 10d; 0
hello,	"H; "e; "l; "l; "o; ",; " ; "W; "o; "r; "l; "d; "!; 15; 12; 0
start,	cla		/ Load next byte into AC
	tad I index
	sna		/ Check for NUL
	hlt		/ Halt when done
	and (177	/ Clear mark parity bit
	tls		/ Clear flag and send byte: TCF + TPC
	tsf		/ Wait for byte to be sent
	jmp .-1
	jmp start
