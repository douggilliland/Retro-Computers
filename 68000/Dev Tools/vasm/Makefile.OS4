# AmigaOS 4.x/PPC

TARGET = _os4
TARGETEXTENSION = 
OUTFMTS = -DOUTAOUT -DOUTBIN -DOUTELF -DOUTHUNK -DOUTSREC -DOUTTOS -DOUTVOBJ \
          -DOUTXFIL

CC = vc +aosppc
CCOUT = -o 
COPTS = -c -DAMIGA -D__USE_INLINE__ -O1 $(OUTFMTS)

LD = $(CC)
LDOUT = $(CCOUT)
LDFLAGS = -lm

RM = delete force quiet

include make.rules
