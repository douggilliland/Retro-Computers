# PowerUp

TARGET = _pup
TARGETEXTENSION = 
OUTFMTS = -DOUTAOUT -DOUTBIN -DOUTELF -DOUTHUNK -DOUTSREC -DOUTTOS -DOUTVOBJ \
          -DOUTXFIL

CC = vc +powerup
CCOUT = -o 
COPTS = -c -c99 -DAMIGA -O1 $(OUTFMTS)

LD = $(CC)
LDOUT = $(CCOUT)
LDFLAGS = -lm -lamiga

RM = delete force quiet

include make.rules
