<?xml version="1.0" encoding="utf-8" ?>

<!--
Default settings for Retro Assembler.

Best practice is leaving these values intact, especially because updates
of the assembler come with an updated settings file, which may cause
losing your customized settings.

Duplicate this file as "retroassembler-usersettings.xml" and place it
into the same directory where the assembler is. The contents of that file
will also be loaded, and it will not get overwritten by updates.

Just keep the modified Setting lines in there and it will work fine.
-->

<Settings>

	<Setting Name="CpuType" Type="String" Value="6502" />
	<Setting Name="OutputFormat" Type="String" Value="bin" />
	<Setting Name="DefaultScreencode" Type="String" Value="screencode,lower" />

	<Setting Name="IncludePath" Type="String" Value="" />
	<Setting Name="VicePath" Type="String" Value="" />

	<Setting Name="CaseSensitiveMode" Type="Boolean" Value="false" />
	<Setting Name="ShowLabelsAfterCompiling" Type="Boolean" Value="false" />
	<Setting Name="ShowLocalLabelsAfterCompiling" Type="Boolean" Value="false" />
	<Setting Name="TreatWarningsAsErrors" Type="Boolean" Value="false" />
	<Setting Name="AllowUndocumentedInstructions" Type="Boolean" Value="true" />
	<Setting Name="OmitUnusedFunctions" Type="Boolean" Value="false" />
	<Setting Name="HandleLongBranch" Type="Boolean" Value="false" />
	<Setting Name="OutputSaveEntireBanks" Type="Boolean" Value="false" />
	<Setting Name="OutputSaveIndividualSegments" Type="Boolean" Value="false" />
	<Setting Name="OutputSaveInfoFile" Type="Boolean" Value="false" />
	<Setting Name="OutputSaveTimeStamp" Type="Boolean" Value="false" />
	<Setting Name="RandomSeed" Type="Integer" Value="0" />
	<Setting Name="BeforeBuild" Type="String" Value="" />
	<Setting Name="AfterBuild" Type="String" Value="" />
	<Setting Name="Launch" Type="Boolean" Value="false" />
	<Setting Name="LaunchCommand" Type="String" Value="" />
	<Setting Name="UpdateCheck" Type="Boolean" Value="true" />

	<Setting Name="OutputTxtAddressFormatFirst" Type="String" Value="{0:x04} " />
